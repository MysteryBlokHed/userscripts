  },
  "devDependencies": {
    "@types/greasemonkey": "^4.0.2",
    "greasetools": "^0.5.0",
    "ls-proxy": "^0.4.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.5.1",
    "typescript": "^4.5.3"
  }
}
