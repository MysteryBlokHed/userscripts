{
  "$schema": "https://json.schemastore.org/package",
  "name": "userscripts",
  "version": "0.1.0",
  "description": "GreaseMonkey UserScripts I've written",
  "private": true,
  "repository": "https://gitlab.com/MysteryBlokHed/userscripts",
